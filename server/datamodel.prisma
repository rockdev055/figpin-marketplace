type User {
  id: ID! @id
  name: String!
  email: String!
  userName: String!
}

type Pin {
  id: ID! @id
  name: String!
  figpinID: Int!
  exclusive: Boolean
  image: String
  largeImage: String
  edition: String!
  volumnSize: Int!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
